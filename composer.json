{
    "name": "nenglish7/security-traits",
    "description": "A bridge between Genial and with the Pusher REST API.",
    "keywords": [
        "pusher-http-php",
        "pusher-bridge",
        "rest-api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Pusher Bridge Contributors",
            "homepage": "https://github.com/genial-framework/pusher-bridge/graphs/contributors",
            "role": "Contribute"
        }
    ],
    "autoload": {
        "psr-4": {
            "Genial\\Pusher\\": "./src"
        }
    },
    "require": {
        "php": ">=7.2",
        "pusher/pusher-php-server": "^3",
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "vimeo/psalm": "^1"
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "prefer-stable": true
}
